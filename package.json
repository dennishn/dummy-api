{
  "name": "dummy-api",
  "version": "0.0.0",
  "description": "",
  "author": {
    "name": "Dennis Haulund Nielsen"
  },
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index -w 'src/**/*' --ext 'js json'",
    "debug": "nodemon index --debug -w 'src/**/*' --ext 'js json'",
    "lint": "jshint src/**/*.js",
    "audit": "nsp audit-shrinkwrap && nsp audit-package",
    "missing": "npm ls --depth 1",
    "outdated": "npm outdated --depth 0",
    "prepush": "npm shrinkwrap && npm test",
    "postmerge": "npm install",
    "pretest": "npm run lint"
  },
  "pre-commit": [
    "lint",
    "audit",
    "missing",
    "outdated"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dennishn/dummy-api"
  },
  "files": [
    "lib",
    "index.js",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "bcryptjs": "^2.1.0",
    "body-parser": "^1.12.2",
    "colors": "^1.0.3",
    "cors": "^2.5.3",
    "express": "^4.12.3",
    "faker": "^2.1.2",
    "glob": "^5.0.3",
    "helmet": "^0.7.0",
    "jsonwebtoken": "^4.2.1",
    "lodash": "^3.5.0",
    "method-override": "^2.3.2",
    "mongoose": "^4.0.1",
    "morgan": "^1.5.2",
    "multer": "^0.1.8",
    "paginate-for-mongoose": "^1.0.1",
    "passport": "^0.2.1",
    "passport-local": "^1.0.0",
    "path": "^0.11.14",
    "redis": "^0.12.1"
  },
  "devDependencies": {
    "nsp": "^1.0.0",
    "jshint": "latest",
    "nodemon": "^1.3.7"
  },
  "license": {
    "type": "MIT"
  },
  "engines": {
    "node": ">=0.10",
    "npm": ">=2.x"
  }
}
